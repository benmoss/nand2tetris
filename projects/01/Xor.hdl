/**
 *  Exclusive-or gate: out = !(a == b).
 */

CHIP Xor {

    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=nota);
    Not(in=b, out=notb);
    Nand(a=nota, b=b, out=x);
    Nand(a=a, b=notb, out=y);
    And(a=x, b=y, out=z);
    Not(in=z, out=out);
}
